/*________________ MIXIN-DECLARATION____________________*/
@mixin gradB(){
	background:
linear-gradient(27deg, #151515 5px, transparent 5px) 0 5px,
linear-gradient(207deg, #151515 5px, transparent 5px) 10px 0px,
linear-gradient(27deg, #222 5px, transparent 5px) 0px 10px,
linear-gradient(207deg, #222 5px, transparent 5px) 10px 5px,
linear-gradient(90deg, #1b1b1b 10px, transparent 10px),
linear-gradient(#1d1d1d 25%, #1a1a1a 25%, #1a1a1a 50%, transparent 50%, transparent 75%, #242424 75%, #242424);
background-color: #131313;
background-size: 20px 20px;

}

@mixin corner(){
	width:200px;
	height:200px;
	vertical-align: top;
	opacity:0.9;
}
@mixin smallcorner(){
	width:100px;
	height:100px;
	vertical-align: top;
	opacity:0.9;
}
/*------------------------------ mixin end---*/
/*________________ MEDIA QUERIES____________________*/

/*///////////// Ideal for tabs and desktops \\\\\\\\\\\\\\\\*/
@media (min-width:786px){


}
/******************* desktop end ***********************/
/*///////////// Ideal for Iphone6 and smart phones \\\\\\\\\\\\\\\\*/
@media (min-width:500px) and (max-width:785px){
	.brandName{width:400px;}

}
/******************* i6 end *****************/
/*///////////// Ideal for small phones like apple 4 \\\\\\\\\\\\\\\\*/
@media (max-width:499px){
.cl{@include smallcorner();}
.cr{@include smallcorner();}
.brandName{width:300px;}
}
/********************small phone end*********************/

/*------------------------------ media queries end---*/

